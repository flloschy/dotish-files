{
    "editor.fontFamily": "'JetBrainsMono Nerd Font', monospace",
    "editor.fontLigatures": true,
    "svelte.enable-ts-plugin": true,
    "dart.flutterSdkPath": "/home/floschy/custominstall/flutter",
    "workbench.colorTheme": "Material Code",
    "editor.renderWhitespace": "trailing",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.detectIndentation": false,
    "editor.experimentalWhitespaceRendering": "font",
    "editor.renderLineHighlight": "gutter",
    "editor.smoothScrolling": true,
    "editor.tabCompletion": "on",
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorWidth": 3,
    "editor.minimap.autohide": "mouseover",
    "editor.minimap.renderCharacters": false,
    "editor.minimap.side": "left",
    "editor.inlineSuggest.syntaxHighlightingEnabled": true,
    "editor.suggest.localityBonus": true,
    "editor.suggest.preview": true,
    "files.insertFinalNewline": true,
    "workbench.reduceMotion": "off",
    "workbench.startupEditor": "none",
    "workbench.activityBar.location": "top",
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.tabActionLocation": "left",
    "workbench.editor.tabSizing": "fixed",
    "workbench.editor.tabSizingFixedMaxWidth": 150,
    "window.menuBarVisibility": "toggle",
    "accessibility.dimUnfocused.enabled": true,
    "accessibility.dimUnfocused.opacity": 0.7,
    "accessibility.underlineLinks": true,
    "explorer.incrementalNaming": "smart",
    "explorer.openEditors.sortOrder": "alphabetical",
    "explorer.sortOrderLexicographicOptions": "upper",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.enableImages": true,
    "terminal.integrated.gpuAcceleration": "on",
    "terminal.integrated.minimumContrastRatio": 7,
    "update.mode": "none",
    "chat.commandCenter.enabled": false,
    "workbench.layoutControl.enabled": false,
    "workbench.navigationControl.enabled": false,
    "window.customTitleBarVisibility": "auto",
    "window.dialogStyle": "custom",
    "window.titleBarStyle": "custom",
    "editor.tokenColorCustomizations": {},
    "files.autoSave": "onFocusChange",
    "editor.copyWithSyntaxHighlighting": false,
    "[json]": {
        "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "cSpell.userWords": [
        "barbg",
        "barfg",
        "blurhash",
        "Brainz",
        "Chaphasilor",
        "configurationpage",
        "darkred",
        "deno",
        "denoland",
        "devin",
        "downloadurl",
        "finamp",
        "Finamp",
        "flac",
        "Floschy",
        "fullopacity",
        "hitsong",
        "jellyfin",
        "Jellyfin",
        "jetlag",
        "mbid",
        "mensa",
        "nginx",
        "refetched",
        "rowid",
        "tabler",
        "tailwindcss",
        "unessesary",
        "webma"
    ],
    "diffEditor.ignoreTrimWhitespace": false,
    "explorer.confirmDelete": false,
    "workbench.editor.empty.hint": "hidden",
    "cSpell.language": "en,de-DE",
    "clangd.path": "/home/floschy/.config/VSCodium/User/globalStorage/llvm-vs-code-extensions.vscode-clangd/install/21.1.0/clangd_21.1.0/bin/clangd",
    "github.copilot.chat.agent.runTasks": false,
    "github.copilot.chat.agent.autoFix": false,
    "typescript.tsserver.watchOptions": "vscode",
    "editor.semanticHighlighting.enabled": true,

    // color theme
    "material-code.colors": {
        // All fields are optional. Unset colors are derived from primary
        "primary": "{{colors.primary.default.hex}}",

        "foreground": "{{colors.on_surface.default.hex}}",
        "mutedForeground": "{{colors.on_surface_variant.default.hex}}",
        "background": "{{colors.surface.default.hex}}",
        // Elevated panel
        "card": "{{colors.surface_container.default.hex}}",
        // Dialog, dropdown
        "popover": "{{colors.surface_container_high.default.hex}}",
        // Hover, selected
        "hover": "{{colors.surface_container_highest.default.hex}}",
        // Input border, divider
        "border": "{{colors.outline_variant.default.hex}}",
        "primaryForeground": "{{colors.on_primary.default.hex}}",
        // Tonal button, tooltip
        "secondary": "{{colors.secondary_container.default.hex}}",
        "secondaryForeground": "{{colors.on_secondary_container.default.hex}}",
        "error": "{{colors.error_container.default.hex | lighten: 10 }}",
        "errorForeground": "{{colors.on_error.default.hex}}",
        // Success indicators, green terminal colors
        "success": "{{colors.primary.default.hex | set_hue: 109 }}",
        // Warning indicators, yellow terminal colors
        "warning": "#d9e014AA",

        "syntax.comment": "{{colors.on_background.default.hex | set_saturation: 11.0 | set_lightness: 56 }}",
        "syntax.string": "{{colors.on_tertiary_container.default.hex | set_saturation: 43.0 | set_lightness: 75 }}",
        // Keywords, operators, control flow
        "syntax.keyword": "{{colors.primary.default.hex | set_saturation: 61.0 | set_lightness: 67 }}",
        // Variables, tags, HTML elements
        "syntax.variable": "{{colors.secondary.default.hex | set_saturation: 68.0| set_lightness: 98 }}",
        // HTML attributes, CSS selectors
        "syntax.attribute": "{{colors.on_tertiary_fixed_variant.default.hex | set_saturation: 68.0| set_lightness: 98 }}",
        // Object properties, CSS properties
        "syntax.property": "{{colors.surface_tint.default.hex | set_saturation: 82.0| set_lightness: 95 }}",
        // Functions, methods, CSS values
        "syntax.function": "{{colors.tertiary.default.hex | set_saturation: 40.0| set_lightness: 56 }}",
        // Numbers, constants, types
        "syntax.constant": "{{colors.source_color.default.hex | set_saturation: 48.0| set_lightness: 75 }}",
        // Bracket pair colors
        "syntax.bracket1": "{{colors.primary_container.default.hex | set_saturation: 5.0| set_lightness: 95 }}",
        "syntax.bracket2": "{{colors.primary_container.default.hex | set_saturation: 5.0| set_lightness: 95 }}",
        "syntax.bracket3": "{{colors.primary_container.default.hex | set_saturation: 5.0| set_lightness: 95 }}",
        "syntax.bracket4": "{{colors.primary_container.default.hex | set_saturation: 5.0| set_lightness: 95 }}"
    }
}
